<div class="form-container">
  <%= bootstrap_form_for @video do |f| %>
      <%= f.text_field :youtube_id, label: "YouTube ID" %>
      <%= f.text_field :title %>
      <%= f.text_field :album %>
      <%= f.text_field :featured_artists %>
      <%= f.date_select :released_on, order: [:month, :day, :year] %>
      <%= f.text_field :tags %>
      <%= f.submit "Submit" %>
  <% end %>
</div>
